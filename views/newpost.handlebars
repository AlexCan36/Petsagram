{{!-- Creates a new post, we can adapt how to add the photo in later --}}
<form class="new-post-form">
  <div>
    <label for="url-new-post">Add the url for the photo you wish to add:</label>
    <input type="text" id="URL-new-post" />
  </div>
  <div>
    <label for="caption-new-post">Caption:</label>
    <input type="text" id="caption-new-post" />
  </div>
  <div>
    <button type="submit">Post!</button>
  </div>
</form>